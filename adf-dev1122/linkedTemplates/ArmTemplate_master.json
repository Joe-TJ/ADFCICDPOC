{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev1122"
		},
		"ADFDEVAKV1122_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akvadfdev.vault.azure.cn/"
		},
		"AzureBlobStorageDestination_properties_typeProperties_connectionString": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ADFDEVAKV1122",
					"type": "LinkedServiceReference"
				},
				"secretName": "blobDestinationConnectionString"
			}
		},
		"AzureBlobStorageSource_properties_typeProperties_connectionString": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ADFDEVAKV1122",
					"type": "LinkedServiceReference"
				},
				"secretName": "blobSourceConnectionString"
			}
		},
		"dataFactory_location": {
			"type": "string",
			"defaultValue": "chinaeast2"
		},
		"dataFactory_identity_type": {
			"type": "string",
			"defaultValue": "SystemAssigned"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev1122_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dataFactory_location": {
						"value": "[parameters('dataFactory_location')]"
					},
					"dataFactory_identity_type": {
						"value": "[parameters('dataFactory_identity_type')]"
					},
					"ADFDEVAKV1122_properties_typeProperties_baseUrl": {
						"value": "[parameters('ADFDEVAKV1122_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorageDestination_properties_typeProperties_connectionString": {
						"value": "[parameters('AzureBlobStorageDestination_properties_typeProperties_connectionString')]"
					},
					"AzureBlobStorageSource_properties_typeProperties_connectionString": {
						"value": "[parameters('AzureBlobStorageSource_properties_typeProperties_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}